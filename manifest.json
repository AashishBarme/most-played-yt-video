{
  "name": "Youtube Most Played Video",
  "version": "1.0",
  "description": "displaying most played youtube video in a week",
  "permissions": ["tabs","storage","declarativeContent"],
  "background": {
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.png",
      "32":"images/icon.png",
      "48":"images/icon.png",
      "128":"images/icon.png"  
    }
  },
  "icons": {
    "16": "images/icon.png",
    "32":"images/icon.png",
    "48":"images/icon.png",
    "128":"images/icon.png"
  },
  "manifest_version": 2
}
